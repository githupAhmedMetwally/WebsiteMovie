@model IEnumerable<ETickets.Models.Cinema>

 
<h1>Cinemas</h1>

 
<table class="table">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
             
            <td>

                    <a href="/Movie/GetCinemas/@item.Id">Get Cinemas</a>
            </td>
        </tr>
}
    </tbody>
</table>
