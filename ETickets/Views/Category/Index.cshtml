@model IEnumerable<ETickets.Models.Category>

 

<h1>Caregories</h1>
 
<table class="table">
    <thead>
        <tr>
             
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
             
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                    <a href="/Movie/GetDetails/@item.Id">Get Moviews</a>
            </td>
        </tr>
}
    </tbody>
</table>
